{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"rootDir": "./src",
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["src/**/*.ts", "src/api/services/errorUtil/errorUtility.ts"],
	"exclude": ["node_modules"]
}
